{
  "name": "nenya-flux",
  "version": "0.1.0",
  "description": "An ES6 implementation of Flux architecture",
  "license": "MIT",
  "keywords": [
    "white ring",
    "nenya",
    "states",
    "flux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WhiteRing/NenyaContext.git"
  },
  "scripts": {
    "clean": "rimraf lib coverage",
    "lint": "eslint src test examples",
    "test": "mocha",
    "test:watch": "npm test -- --watch",
    "test:cov": "./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "eslint": "^1.7.1",
    "eslint-config-rackt": "1.0.0",
    "expect": "^1.8.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "rimraf": "^2.3.4"
  },
  "npmName": "nenya-flux",
  "npmFileMap": [{
      "basePath": "/src/",
      "files": [
        "*.js"
      ]
    }
  ]
}
